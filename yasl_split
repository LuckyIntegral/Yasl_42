#!yasl_linux
!!

{
	[#] 1 != ? ("Separator must be a character\n" print -1 exit)
} check_separator

{
	0:4
	[#]
	=:2 // for nbr of args
	0
	2 ^
	@ (
		%]#[ = 3:=:0 == ? (
			4:=:0 ? ( "\n" print 0:4 ) !
		) : (
			print 1:4
		)
		1 + =
		2:=:0
		!=
	)
	4:=:0 ? ( "\n" print ) !
} printer

# 2 != ? ("Usage: ./yasl_split <separator> <string>\n" print -1 exit)

0:=:2 2:check_separator:2

0:=:3 // for separator

2 ^

printer